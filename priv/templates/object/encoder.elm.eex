<%= encoder_name %> : <%= type_name %> -> Value
<%= encoder_name %> <%= argument_name %> =
    []<%#
    %><%= for property <- properties do %>
        <%= if property.required == true do %><%#
        %>|> Encode.required "<%= property.name %>" <%= argument_name %>.<%= property.name %> <%= property.encoder_name %><%#
      %><% else %><%#
        %>|> Encode.optional "<%= property.name %>" <%= argument_name %>.<%= property.name %> <%= property.encoder_name %><%#
      %><% end %><%#
    %><% end %>
        |> Encode.object
